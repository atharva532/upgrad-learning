    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "react": "^18.3.0",
    "react-dom": "^18.3.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.0",
    "@vitest/coverage-v8": "^4.0.17",
    "jsdom": "^27.4.0",
    "typescript": "^5.4.0",
    "vite": "^6.0.0",
    "vitest": "^4.0.17"
  }
}
